{
  "plugins": [
    "@cypress/dev"
  ],
  "extends": [
    "plugin:@cypress/dev/general",
    "plugin:@cypress/dev/tests"
  ],
  "env": {
    "node": true
  },
  "globals": {
    "cy": true,
    "Cypress": true
  },
  "rules": {
    "strict": [1, "global"],
    "no-undef": 1,
    "no-unused-vars": 1,
    "semi": 0,
    "semi-spacing": 1,
    "no-unexpected-multiline": 1,
    "quotes": [1, "single", "avoid-escape"],
    "indent": [2, 2, { "SwitchCase": 1 }]
  }
}